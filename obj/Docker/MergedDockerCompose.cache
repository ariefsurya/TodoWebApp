      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\arief\Test Kawan Lama\TodoWebApp\TodosApi\Database
        target: /app/Database
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\congl\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\congl\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  todowebapp:
    build:
      context: C:\arief\Test Kawan Lama\TodoWebApp
      dockerfile: TodoWebApp/Dockerfile
    container_name: todowebapp
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: todowebapp
    networks:
      todonetwork: null
